FoamFile
{
    version     2.0;
    format      ascii;
    class       pointVectorField;
    object      pointDisplacement;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    frontandback
    {
        type            empty;
    }

    inlet
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
	
    outlet
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
	
    ground
    {
        type            fixedNormalSlip;
        n               (0 0 1);
        value           uniform (0 0 0);
    }
    atmosphere
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    floatingObject
    {	
		type            calculated;
        value           uniform (0 0 0);
    }

    
}


